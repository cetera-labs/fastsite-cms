    "@sencha/ext-ux": "^7.0.0",
    "ace-builds": "^1.4.12"
  },
  "devDependencies": {
    "@sencha/ext-webpack-plugin": "~7.0.0",
    "html-webpack-plugin": "^3.2.0",
